import crafttweaker.api.item.IItemStack;
import crafttweaker.api.data.MapData;

craftingTable.removeByName("apotheosis:potion_charm");

var exampleShapedRecipeVarFunction as function(usualOut as IItemStack, inputs as IItemStack[][]) as IItemStack = (usualOut as IItemStack, inputs as IItemStack[][]) => {
	var tag = inputs[1][0].tag as MapData;
	if (tag.contains("Potion")) {
		var potion = tag.getAt("Potion");
		return usualOut.withTag({Potion: potion});
	}
    return usualOut;
};


